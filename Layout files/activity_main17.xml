        android:text="My Orders"
        android:layout_marginTop="280dp"
        android:layout_marginStart="90dp"
        android:textColor="#37474F"
        android:textStyle="bold"
        android:textSize="18sp"/>

    <TextView
        android:id="@+id/MyWishlisttext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="My Wishlist"
        android:layout_marginTop="330dp"
        android:layout_marginStart="90dp"
        android:textColor="#37474F"
        android:textStyle="bold"
        android:textSize="18sp"/>

    <TextView
        android:id="@+id/Chatwithustext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chat with us"
        android:layout_marginTop="380dp"
        android:layout_marginStart="90dp"
        android:textColor="#37474F"
        android:textStyle="bold"
        android:textSize="18sp"/>

    <TextView
        android:id="@+id/TalktoourSupporttext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Talk to our Support"
        android:layout_marginTop="430dp"
        android:layout_marginStart="90dp"
        android:textColor="#37474F"
        android:textStyle="bold"
        android:textSize="18sp"/>

    <TextView
        android:id="@+id/searchsomethingtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search Something"
        android:layout_marginTop="480dp"
        android:layout_marginStart="90dp"
        android:textColor="#37474F"
        android:textStyle="bold"
        android:textSize="18sp"/>

